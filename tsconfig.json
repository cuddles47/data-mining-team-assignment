    "compilerOptions": {
      "target": "es2016",
      "module": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "lib": ["dom", "es2016"]
    },
    "include": ["src/**/*", "web/**/*"]
  }
  